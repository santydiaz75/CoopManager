	<queryString>
		<![CDATA[SELECT c.semana, ca.IdCategoria, ca.NombreCategoria,
coalesce([db_aa764d_coopmanagerdb].[dbo].[EntradasKilosSemana]($P{Empresa}, $P{Ejercicio}, c.semana, ca.IdCategoria),0) as EntradaKilosSemana,
coalesce([db_aa764d_coopmanagerdb].[dbo].[VentasKilosSemana]($P{Empresa}, $P{Ejercicio}, c.semana, ca.IdCategoria),0) as VentasKilosSemana,
[db_aa764d_coopmanagerdb].[dbo].[EntradasGetFecha]($P{Empresa}, $P{Ejercicio}, c.semana) as Fecha,
coalesce([db_aa764d_coopmanagerdb].[dbo].[EntradasKilosInutilizadosSemana]($P{Empresa}, $P{Ejercicio}, c.semana, ca.IdCategoria),0) as KilosInutilizadosSemana
FROM [db_aa764d_coopmanagerdb].[dbo].[calendario] c cross join [db_aa764d_coopmanagerdb].[dbo].[categorias] ca on  ca.ejercicio = c.ejercicio and ca.empresa = c.empresa
where c.semana >= $P{SemanaDesde} and c.semana <= $P{SemanaHasta} and c.ejercicio = $P{Ejercicio} and c.empresa = $P{Empresa} and
(coalesce([db_aa764d_coopmanagerdb].[dbo].[EntradasKilosSemana]($P{Empresa}, $P{Ejercicio}, c.semana, ca.IdCategoria),0) > 0 or
coalesce([db_aa764d_coopmanagerdb].[dbo].[VentasKilosSemana]($P{Empresa}, $P{Ejercicio}, c.semana, ca.IdCategoria),0) > 0)
group by c.semana, ca.IdCategoria, ca.NombreCategoria;]]>
