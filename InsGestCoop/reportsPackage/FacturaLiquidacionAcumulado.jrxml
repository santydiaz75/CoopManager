	<queryString>
		<![CDATA[SELECT NombreCategoria, Orden,
`coopmanagerdb`.LiquidacionKilosAcumulados($P{Empresa}, $P{Ejercicio}, $P{SemanaHasta}, $P{Grupo}, c.IdCategoria) as KilosAcumulados,
