	<queryString>
		<![CDATA[SELECT
     facturascabecera.[Empresa] AS facturascabecera_Empresa,
     [db_aa764d_coopmanagerdb].[dbo].[empresas].[NIF] AS empresas_NIF,
     [db_aa764d_coopmanagerdb].[dbo].[empresas].[Direccion] AS empresas_Direccion,
     [db_aa764d_coopmanagerdb].[dbo].[empresas].[Poblacion] AS empresas_Poblacion,
     [db_aa764d_coopmanagerdb].[dbo].[empresas].[CodigoPostal] AS empresas_CodigoPostal,
     (coalesce([db_aa764d_coopmanagerdb].[dbo].[empresas].[CodigoPostal] + "', '   '), coalesce([db_aa764d_coopmanagerdb].[dbo].[empresas].[Poblacion],'')) AS empresas_CPPoblacion,
     [db_aa764d_coopmanagerdb].[dbo].[empresas].[Provincia] AS empresas_Provincia,
     [db_aa764d_coopmanagerdb].[dbo].[empresas].[CorreoElectronico] AS empresas_CorreoElectronico,
     [db_aa764d_coopmanagerdb].[dbo].[empresas].[Telefono] AS empresas_Telefono,
     [db_aa764d_coopmanagerdb].[dbo].[empresas].[Fax] AS empresas_Fax,
     facturascabecera.[Ejercicio] AS facturascabecera_Ejercicio,
     facturascabecera.[IdFactura] AS facturascabecera_IdFactura,
     facturascabecera.[Semana] AS facturascabecera_Semana,
     facturascabecera.[Fecha] AS facturascabecera_Fecha,
     facturascabecera.[CuentaCliente] AS facturascabecera_CuentaCliente,
     facturascabecera.[Nif] AS facturascabecera_Nif,
     facturascabecera.[Nombre] AS facturascabecera_Nombre,
     facturascabecera.[Direccion] AS facturascabecera_Direccion,
     facturascabecera.[Poblacion] AS facturascabecera_Poblacion,
     facturascabecera.[Provincia] AS facturascabecera_Provincia,
     facturascabecera.[CodigoPostal] AS facturascabecera_CodigoPostal,
     (coalesce(facturascabecera.[CodigoPostal] + '', '   '), coalesce(facturascabecera.[Poblacion],       '')) AS facturascabecera_CPPoblacion,
     facturascabecera.[BaseImponible] AS facturascabecera_BaseImponible,
     facturascabecera.[TipoImpuesto] AS facturascabecera_TipoImpuesto,
     facturascabecera.[ImporteImpuesto] AS facturascabecera_ImporteImpuesto,
     facturascabecera.[ImporteFactura] AS facturascabecera_ImporteFactura,
     [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].[Linea] AS facturaslineas_Linea,
     [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].[Unidades] AS facturaslineas_Unidades,
     [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].[Concepto] AS facturaslineas_Concepto,
     [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].[Precio] AS facturaslineas_Precio,
     [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].[PctImpuesto] AS facturaslineas_PctImpuesto,
     [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].[Importe] AS facturaslineas_Importe,
    coalesce([db_aa764d_coopmanagerdb].[dbo].[conceptos].[ConIGIC], 1)  AS ConceptosConIGIC,
     coalesce([db_aa764d_coopmanagerdb].[dbo].[conceptos].[IgnorarUnidades], 0) AS 		      ConceptosIgnorarUnidades,
     [db_aa764d_coopmanagerdb].[dbo].[empresas].Lopd
FROM [[db_aa764d_coopmanagerdb].[dbo].[facturaslineas]] [db_aa764d_coopmanagerdb].[dbo].[facturaslineas]
RIGHT OUTER JOIN [facturascabecera] facturascabecera ON [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].[Empresa] = facturascabecera.[Empresa] INNER JOIN [[db_aa764d_coopmanagerdb].[dbo].[empresas]]  [db_aa764d_coopmanagerdb].[dbo].[empresas] On [db_aa764d_coopmanagerdb].[dbo].[empresas].[IdEmpresa]  = facturascabecera.[Empresa]
     AND [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].[Ejercicio] = facturascabecera.[Ejercicio]
     AND [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].[IdFactura] = facturascabecera.[IdFactura] LEFT OUTER JOIN [[db_aa764d_coopmanagerdb].[dbo].[conceptos]] [db_aa764d_coopmanagerdb].[dbo].[conceptos]  On [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].[Empresa]  = [db_aa764d_coopmanagerdb].[dbo].[conceptos].[Empresa]
     AND [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].[Ejercicio] = [db_aa764d_coopmanagerdb].[dbo].[conceptos].[Ejercicio]
     AND [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].[IdConcepto] = [db_aa764d_coopmanagerdb].[dbo].[conceptos].[Concepto]
WHERE
     $P{Empresa} = facturascabecera.[Empresa]
 AND $P{Ejercicio} = facturascabecera.[Ejercicio]
 AND $P{IdFacturaDesde} <= facturascabecera.[IdFactura]
 AND $P{IdFacturaHasta} >= facturascabecera.[IdFactura]
Order by facturascabecera.[Empresa], facturascabecera.[Ejercicio], facturascabecera.[IdFactura], ConceptosConIGIC Desc, [db_aa764d_coopmanagerdb].[dbo].[facturaslineas].Linea ASC]]>
	</queryString>
